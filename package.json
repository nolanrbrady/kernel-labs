{
  "name": "deepml-sr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build:frontend-client": "node scripts/build-frontend-client.mjs",
    "prelint": "npm run build:frontend-client",
    "lint": "node scripts/lint.mjs && tsc --noEmit",
    "prestart": "npm run build:frontend-client",
    "start": "node --import tsx src/backend/start.ts",
    "pretest:unit": "npm run build:frontend-client",
    "test:unit": "node scripts/run-unit-tests.mjs",
    "pretest:e2e": "npm run build:frontend-client",
    "test:e2e": "playwright test e2e --reporter=line",
    "test": "npm run test:unit && npm run test:e2e",
    "quality-gate": "./scripts/quality_gate.sh"
  },
  "dependencies": {
    "ace-builds": "^1.43.6",
    "express": "^5.1.0",
    "katex": "^0.16.28",
    "markdown-it": "^14.1.1",
    "pdf-parse": "^2.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/express": "^5.0.3",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
