{
  "name": "deepml-sr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "node scripts/lint.mjs && tsc --noEmit",
    "start": "node --import tsx src/backend/start.ts",
    "test:unit": "node --import tsx --test tests/**/*.test.js tests/**/*.test.ts tests/**/*.test.tsx",
    "test:e2e": "playwright test e2e --reporter=line",
    "test": "npm run test:unit && npm run test:e2e",
    "quality-gate": "./scripts/quality_gate.sh"
  },
  "dependencies": {
    "express": "^5.1.0",
    "katex": "^0.16.28",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
